	"global-settings": {
		"favorite-color": {
			"name": "Favorite Color",
			"required": false,
			"allow-project-overrides": false,
			"type": "text",
			"default": "Blue"
		},
		"preferred-transportation": {
			"name": "Preferred Method of Transportation",
			"required": true,
			"allow-project-overrides": true,
			"type": "radio",
