{
	"name": "Example Module",
	
	"permissions": [
		"hook_every_page_top",
		"select_data",
		"update_data",
		"delete_data",
		"update_user_permissions"
	],

	"links": {
		"control-center": [
			{
				"name": "My Control Center Menu Link",
				"icon": "user",
				"url": "system_level_homepage.php"
			}
		],
		"project": [
			{
				"name": "My Project Menu Link",
				"icon": "report",
				"url": "project_homepage.php"
			}
		]
