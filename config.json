				{ "value": "car", "name": "Car" },
				{ "value": "bus", "name": "Bus" },
				{ "value": "bike", "name": "Bicycle" }
			]
		}
	},
	
	"project-settings": {
		"literate": {
			"name": "I certify that I am literate",
			"required": true,
			"type": "checkbox"
		},
		"likes-pizza": {
			"name": "Do you like pizza?",
			"required": false,
			"type": "dropdown",
			"choices": [
				{ "value": "1", "name": "Yes" },
				{ "value": "2", "name": "No" }
			]
		}
	}
