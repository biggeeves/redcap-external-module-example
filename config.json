	"name": "Example Module",
	
	"permissions": [
		"hook_project_home_page",
		"select_data",
		"update_data",
		"delete_data",
		"update_user_permissions"
	],

	"links": {
		"control-center": {
			"My Control Center Menu Link": {
				"icon": "user",
				"url": "system_level_homepage.php"
			}
		},
		"project": {
			"My Project Menu Link": {
				"icon": "report",
				"url": "project_homepage.php"
			}
		}
