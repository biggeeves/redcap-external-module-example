			"allow-project-overrides": true,
			"type": "text",
			"default": "#e2ca9c"
		},
		{
			"key": "preferred-transportation",
			"name": "Preferred Method of Transportation",
			"required": true,
			"allow-project-overrides": false,
			"type": "radio",
			"default": "car",
			"choices": [
				{ "value": "car", "name": "Car" },
				{ "value": "bus", "name": "Bus" },
				{ "value": "bike", "name": "Bicycle" }
			]
		}
	],
	
	"project-settings": [
		{
			"key": "literate",
			"name": "I certify that I am literate",
			"required": true,
			"type": "checkbox"
		},
		{
			"key": "likes-pizza",
			"name": "Do you like pizza?",
			"required": false,
			"type": "dropdown",
			"choices": [
				{ "value": "1", "name": "Yes" },
				{ "value": "2", "name": "No" }
			]
		}
	]
